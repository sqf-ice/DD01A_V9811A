 189          //---------------------------------------------------------------------//
 190          void Lib_MVHalfByte_R(INT8U *addr,INT8U numbyte)
 191          {
 192   1              INT8U i;
 193   1      
 194   1      
 195   1              for(i=numbyte-1;i>0;i--)
 196   1              {
 197   2                      addr[i] = ((addr[i]>>4) & 0x0f) + ((addr[i-1]<<4) & 0xf0);
 198   2              }
 199   1              addr[0] = (addr[0]>>4) & 0x0f;
 200   1      
 201   1      }
